<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <com.jenzz.materialpreference.PreferenceCategory
        android:key="@string/key_preference_category_lock"
        android:title="@string/setting_lock">

        <com.jenzz.materialpreference.Preference
            android:key="@string/key_lock_style"
            android:summary="默认为滑动解锁"
            android:title="@string/setting_lock_style" />
    </com.jenzz.materialpreference.PreferenceCategory>

    <com.jenzz.materialpreference.PreferenceCategory
        android:title="@string/title_setting_base">
        <com.jenzz.materialpreference.SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_shake_feed_back"
            android:title="@string/setting_shake_feed_back"
            android:summary="@string/setting_shake_feed_back_summary"/>
        <com.jenzz.materialpreference.Preference
            android:key="@string/key_clear_cache"
            android:title="@string/setting_clear_cache"
            android:summary="@string/setting_clear_cache_summary"/>

    </com.jenzz.materialpreference.PreferenceCategory>
</PreferenceScreen>